{% extends 'base.html.twig' %}

{% block title %}Service n°{{service.codeService}}{% endblock %}

{% block body %}
<div class="achat-container">
    <div class='align-items-center justify-content-center d-flex mt-3 bandeau-form-search'>
        <h5 class="mt-4 ms-3 blueF">PFC N°{{service.codeService}}</h5>
        <div class="bande ms-3">
        </div>
    </div>
    <div class="action-container p-3">
    <table class="">
        <tbody>
            <tr>
                <th>Code PFC</th>
                <td>{{ service.codeService }}</td>
            </tr>
            <tr>
                <th>Nom PFC</th>
                <td>{{ service.nomService }}</td>
            </tr>
      </tbody>
    </table>
    <div class="align-items-center justify-content-center d-flex mt-3">

    <a class="fr-btn me-3" href="{{ path('app_services_edit', {'id': service.id}) }}">Modifier</a>

    {# {{ include('services/_delete_form.html.twig') }} #}
    </div>
    <div class="align-items-center justify-content-center d-flex mt-3">
        <a class="fr-btn" href="{{ path('app_services_index') }}">Retour à la liste</a>
</div>
    </div>
    </div>

</div>
{% endblock %}
